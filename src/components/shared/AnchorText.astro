---
import type { HTMLAttributes } from 'astro/types';

interface Props extends HTMLAttributes<'a'> {}
const props = Astro.props;
---

<a
  class="border-b border-gray-300 text-gray-900 hover:border-gray-500 dark:border-gray-600 dark:text-gray-100 dark:hover:border-gray-400"
  {...props}><slot /></a
>
